{
  "name": "@testmind/cli",
  "version": "0.5.0-beta",
  "description": "Command-line interface for TestMind",
  "main": "./dist/index.js",
  "bin": {
    "testmind": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/cli.ts src/index.ts --format cjs",
    "dev": "tsup src/cli.ts src/index.ts --format cjs --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@testmind/shared": "workspace:*",
    "@testmind/core": "workspace:*",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "cli-table3": "^0.6.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}







